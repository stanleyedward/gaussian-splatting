{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: train.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/train.py",
      "args": [
        "--source_path",
        "${workspaceFolder}/lego",
        // "--source_path",
        // "${workspaceFolder}/tandt_db/db/playroom",
        "--iterations",
        "7000",
        "--save_iterations",
        "7000",
        "--test_iterations",
        "7000",
        "--debug_from",
        "-1",
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}"
    }
  ]
}